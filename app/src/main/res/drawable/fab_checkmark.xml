<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="32dp"
        android:height="8dp"
        android:top="20dp">
        <rotate
            android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@color/fab_checkmark"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="50dp"
        android:height="8dp"
        android:top="15dp"
        android:left="16dp">
        <rotate
            android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="@color/fab_checkmark"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="10dp"
        android:height="40dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
</layer-list>